      <pattern>DAinfo</pattern>
      <template>
          <link>
              <text>Find info about Database Administrator careers here</text>
              <url>https://www.computerscience.org/careers/database-administrator/</url>
          </link>
      </template>
  </category>
  
    <category>
      <pattern>DATA</pattern>
